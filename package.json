{
  "name": "geo-weather-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./scripts/start.js && cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run start:server\" \"npm run start:client\"",
    "server": "nodemon ./scripts/start",
    "client": "cd client && npm start",
    "postinstall": "npm install --prefix client",
    "heroku-postbuild": "cd client && npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.1"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ]
}
